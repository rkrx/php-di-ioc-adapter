{
	"name": "rkr/php-di-ioc-adapter",
	"description": "An adapter for PHP-DI4 and rkr/php-ioc-contract",
	"type": "library",
	"license": "MIT",
	"require": {
		"php": ">= 5.3",
		"rkr/php-ioc-contract": "~0.1@stable",
		"mnapoli/php-di": "~4.4@stable"
	},
	"require-dev": {
		"phpunit/phpunit": "~3.7@stable"
	},
	"autoload": {
		"psr-4": {
			"DIAdapter\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"DIAdapter\\": ["src/", "tests/"]
		}
	}
}
