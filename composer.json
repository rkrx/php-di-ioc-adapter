{
	"name": "rkr/php-di-ioc-adapter",
	"description": "An adapter for PHP-DI5 and rkr/php-ioc-contract",
	"type": "library",
	"license": "MIT",
	"require": {
		"php": ">= 7.1",
		"rkr/php-ioc-contract": "0.1.*@stable",
		"php-di/php-di": "6.*@stable"
	},
	"require-dev": {
		"phpunit/phpunit": "7.*@stable"
	},
	"autoload": {
		"psr-4": {
			"DIAdapter\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"DIAdapter\\": ["src/", "tests/"]
		}
	}
}
