{
	"name": "rkr/php-di-ioc-adapter",
	"description": "An adapter for PHP-DI5 and rkr/php-ioc-contract",
	"type": "library",
	"license": "MIT",
	"require": {
		"php": ">= 8.0",
		"rkr/php-ioc-contract": "0.1.*@stable",
		"php-di/php-di": "^7.0@stable",
		"container-interop/container-interop": "^1.2"
	},
	"require-dev": {
		"phpunit/phpunit": "9.*@stable",
		"phpstan/phpstan": ">= 0.1"
	},
	"autoload": {
		"psr-4": {
			"DIAdapter\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"DIAdapter\\": ["src/", "tests/"]
		}
	},
	"scripts": {
		"phpstan": "phpstan analyse -l max src"
	}
}
